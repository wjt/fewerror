{
  "version": 1,
  "formatters": {
    "simple": {
      "format": "%(asctime)s %(levelname)8s [%(name)s] %(message)s"
    }
  },
  "handlers": {
    "file": {
      "class": "logging.FileHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "filename": "log/fewerror.log"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": ["file"]
  }
}
